{
  "name": "@rockstat/front",
  "version": "3.2.1",
  "description": "Rockstat platform server endpoint frontier",
  "main": "dist/start.js",
  "scripts": {
    "start": "ts-node -r 'tsconfig-paths/register'  ./src/start.ts | pino",
    "start:prod": "node -r 'source-map-support/register' dist/start",
    "start:dev": "ts-node-dev -r 'tsconfig-paths/register' -r 'source-map-support/register' ./src/start.ts | pino",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json -t stylish -p ./tsconfig.json",
    "security-check": "nsp check",
    "watch": "tsc -w",
    "tun": "lt --port 10001 -s rstdev",
    "build:push": "git add dist/* && git add package.json && git commit -m build && git push origin HEAD",
    "build:patch": "yarn run patch && git add package.json",
    "patch": "yarn version --loose-semver --new-version patch"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rockstat/frontier"
  },
  "author": "Dmitry Rodin <madiedinro@gmail.com>",
  "homepage": "https://rockstat.ru",
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/cookie": "^0.3.1",
    "@types/get-value": "^3.0.0",
    "@types/micro": "^7.3.1",
    "@types/node": "^9.6.18",
    "@types/pino": "^4.16.0",
    "@types/qs": "^6.5.1",
    "@types/statsd-client": "^0.4.0",
    "@types/ws": "^6.0.0",
    "localtunnel": "^1.9.0",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.21",
    "tsc-watch": "^1.0.17",
    "tsconfig-paths": "^3.3.2",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@rockstat/rock-me-ts": "^1",
    "ajv": "^6.4.0",
    "bluebird": "^3.5.1",
    "cookie": "^0.3.1",
    "eventemitter3": "^3.1.0",
    "find-my-way": "^1.11.1",
    "get-value": "^3.0.1",
    "micro": "^9.1.4",
    "qs": "^6.5.1",
    "reflect-metadata": "^0.1.12",
    "rpc-websockets": "^4.0.2",
    "source-map-support": "^0.5.5",
    "typedi": "^0.8.0",
    "ws": "^6.0.0"
  }
}
